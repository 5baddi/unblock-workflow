{
  "name": "unblock-workflow",
  "version": "1.0.0",
  "description": "Unblock workflow studio",
  "author": "5baddi <project@baddi.info>",
  "private": true,
  "proxy": "http://localhost:3001/",
  "homepage": ".",
  "main": "build/server.js",
  "scripts": {
    "client:install": "cd client && yarn install",
    "client:watch": "cd client && yarn run watch",
    "clean": "shx rm -rf ./build/* ./public/*",
    "compile": "tsc",
    "prod": "cd client && yarn run start",
    "build": "run-s clean compile prod",
    "start": "node -r dotenv/config ./build/server.js",
    "watch": "nodemon -r node_modules/dotenv/config ./build/server.js -e ts,tsx --exec \"yarn run build && yarn run start\""
  },
  "dependencies": {
    "axios": "^0.26.0",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "deep-object-diff": "^1.1.7",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-myconnection": "^1.0.4",
    "inversify": "^6.0.1",
    "inversify-express-utils": "^6.4.3",
    "mongodb": "^4.3.1",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "reflect-metadata": "^0.1.13",
    "tripetto": "^4.4.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "winston": "^3.5.1",
    "winston-daily-rotate-file": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "dotenv": "^16.0.0",
    "npx": "^10.2.2",
    "shx": "^0.3.4",
    "typescript": "^4.5.5",
    "yarn-run-all": "^3.1.1"
  }
}
